            "type": "BOOLEAN",
            "defaultValue" : false
        },
        {
            "name": "use_date",
            "type": "BOOLEAN",
            "defaultValue" : false
        },
        {
            "name": "use_datetime_tz",
            "type": "BOOLEAN",
            "defaultValue" : false
        },
        {
            "name": "use_datetime_no_tz",
            "type": "BOOLEAN",
            "defaultValue" : false
        },
        {
            "name": "export_schema",
            "type": "BOOLEAN",
            "defaultValue" : false
        }
    ]
